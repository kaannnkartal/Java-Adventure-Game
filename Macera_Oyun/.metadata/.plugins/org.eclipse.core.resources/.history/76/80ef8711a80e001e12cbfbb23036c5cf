
public abstract class Character {

	protected int damage,healthy,money,id;
	
	
	public Character(int damage, int healthy, int money, int id) {
		this.damage = damage;
		this.healthy = healthy;
		this.money = money;
		this.id = id;
	}

	
	
	public int getDamage() {
		return damage;
	}

	public void setDamage(int damage) {
		this.damage = damage;
	}

	public int getHealthy() {
		return healthy;
	}

	public void setHealthy(int healthy) {
		this.healthy = healthy;
	}

	public int getMoney() {
		return money;
	}

	public void setMoney(int money) {
		this.money = money;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	
	
	
	
	
	
	
}
