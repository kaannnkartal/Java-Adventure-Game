
public class Tabanca extends Weapon {

	protected  String weaponType = "tabanca";
	protected  int damage, price;
	
	
	Tabanca(weaponType = "tabanca", damage = 2, price=25) {
		super(weaponType,damage,price);
	}


	public String getWeaponType() {
		return weaponType;
	}


	public void setWeaponType(String weaponType) {
		this.weaponType = weaponType;
	}


	public int getDamage() {
		return damage;
	}


	public void setDamage(int damage) {
		this.damage = damage;
	}


	public int getPrice() {
		return price;
	}


	public void setPrice(int price) {
		this.price = price;
	}
	
	
	

	
}
